# test-simple-repo Environment Configuration
# Copy this file to .env and fill in the values

# Application
APP_NAME=test-simple-repo
APP_ENV=development
APP_DEBUG=true
APP_PORT=8000

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/test_simple_repo

# Authentication
JWT_SECRET=your-secret-key-here
JWT_EXPIRY=3600

# External Services
# API_KEY=your-api-key
# WEBHOOK_SECRET=your-webhook-secret

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json
